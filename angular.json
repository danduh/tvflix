{
  "version": 1,
  "projects": {
    "cast-gql": {
      "root": "apps/cast-gql",
      "sourceRoot": "apps/cast-gql/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/cast-gql",
            "main": "apps/cast-gql/src/main.ts",
            "tsConfig": "apps/cast-gql/tsconfig.app.json",
            "assets": [
              "apps/cast-gql/src/assets",
              {
                "glob": "**/*",
                "input": "apps/cast-grpc-ms/src/assets/proto",
                "output": "assets/proto"
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/cast-gql/src/environments/environment.ts",
                  "with": "apps/cast-gql/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "cast-gql:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/cast-gql/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/cast-gql"],
          "options": {
            "jestConfig": "apps/cast-gql/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
