{
  "version": 1,
  "projects": {
    "db-layer": {
      "root": "libs/db-layer",
      "sourceRoot": "libs/db-layer/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/db-layer/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/db-layer"
          ],
          "options": {
            "jestConfig": "libs/db-layer/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "movies-ms": {
      "root": "apps/movies-ms",
      "sourceRoot": "apps/movies-ms/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/movies-ms",
            "main": "apps/movies-ms/src/main.ts",
            "tsPlugins": [
              {
                "name": "@nestjs/swagger/plugin",
                "options": {
                  "dtoFileNameSuffix": [
                    ".dto.ts",
                    ".vm.ts"
                  ]
                }
              }
            ],
            "tsConfig": "apps/movies-ms/tsconfig.app.json",
            "assets": [
              "apps/movies-ms/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/movies-ms/src/environments/environment.ts",
                  "with": "apps/movies-ms/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "movies-ms:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/movies-ms/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/movies-ms"
          ],
          "options": {
            "jestConfig": "apps/movies-ms/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "tvflix": {
      "projectType": "application",
      "root": "apps/tvflix",
      "sourceRoot": "apps/tvflix/src",
      "prefix": "tvflix",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/tvflix",
            "index": "apps/tvflix/src/index.html",
            "main": "apps/tvflix/src/main.ts",
            "polyfills": "apps/tvflix/src/polyfills.ts",
            "tsConfig": "apps/tvflix/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/tvflix/src/favicon.ico",
              "apps/tvflix/src/assets"
            ],
            "styles": [
              "apps/tvflix/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/tvflix/src/environments/environment.ts",
                  "with": "apps/tvflix/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "tvflix:build:production"
            },
            "development": {
              "browserTarget": "tvflix:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tvflix:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tvflix/src/**/*.ts",
              "apps/tvflix/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/tvflix"
          ],
          "options": {
            "jestConfig": "apps/tvflix/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "tvflix-e2e": {
      "root": "apps/tvflix-e2e",
      "sourceRoot": "apps/tvflix-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tvflix-e2e/cypress.json",
            "devServerTarget": "tvflix:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tvflix:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/tvflix-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "tvflix"
      ]
    }
  }
}
